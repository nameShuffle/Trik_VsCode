{
  "name": "trikextension",
  "displayName": "TrikExtension",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.sendFileTrik"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.sendFileTrik",
        "title": "Send file to TRIK"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "TRIK Robot",
      "properties": {
        "trik.address": {
          "type": "string",
          "default": "127.0.0.1",
          "description": "TRIK address for connection."
        },
        "trik.port": {
          "type": "number",
          "default": 8888,
          "description": "Port to connect on."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "@types/socket.io-client": "^1.4.32",
    "socket.io": "^2.2.0"
  }
}
